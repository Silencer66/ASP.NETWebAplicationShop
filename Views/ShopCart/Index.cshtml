@model ShopCartViewModel

<div class="container">
    @foreach (var el in Model.shopCart.listShopItems)
    {
        <div class = "alert alert-warning mt-3">
            <b>Aвтомобиль: </b>@el.car.carName<br />
            <b>Цена: </b>@el.car.carPrice.ToString("c")
        </div>
    }
    <hr />
    <div class="btn btn-danger" app-controller="Order"> Оплатить</div>
</div>